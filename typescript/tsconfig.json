{
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "compilerOptions": {
    "strict": true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "build",
    "rootDir": "./",
    "paths": {
      "Pvault/*": [
        "./vault_typescript_sdk/*"
      ]
    },
  }
}
